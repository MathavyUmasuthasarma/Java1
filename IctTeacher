public class Teacher{
    String designation = "Teacher";
    String collegeName = "UKI";
    void work(){
        System.out.println("Teaching");
        
    }}

public class PhysicsTeacher extends Teacher{
        String mainSubject = "Physics";
    
    public static void main(String[] args) {
        PhysicsTeacher phy1 = new PhysicsTeacher();
        System.out.println(phy1.designation);
        System.out.println(phy1.collegeName);
        System.out.println(phy1.mainSubject);
    }}
    
public class IctTeacher extends Teacher {
    String mainSubject = "ICT";
    public static void main(String[] args){
        IctTeacher ict = new IctTeacher();
        System.out.println(ict.designation);
        System.out.println(ict.collegeName);
        System.out.println(ict.mainSubject);
        
    }
}
    

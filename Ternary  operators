public class Main {
  public static void main(String args[]){
  	int num1, num2;
    num1 = 25;
   
   //num1 is not equal to 10, that's why the second variable num1 after colon is assignedto the value
   
    num2  = (num1 ==10) ? 100: 200 ;
    
    System.out.println("num2: " + num2);
    
      //num1 is not equal to 25, that's why the firs variable num2 after colon is assignedto the value
    num2  = (num1 ==25) ? 100: 200 ;
    
    System.out.println("num2: " + num2);
    
    
}
}
